class Main {
  public static void main(String[] args) {
     int i = 0;
    int j = 0;
    System.out.println("* | 1 2 3 4 5 6 7 8 9");
    System.out.println("_______________________");
    for (i=1; i<10; i++) {
    System.out.print(i + "  |");
      for (j = 1; j<10; j++) {
        System.out.print((i*j)+ " ");
        }
      System.out.println();
      }
      System.out.println();

    }
  }
